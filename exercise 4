import java.util.ArrayList;

public class Main {
    public static void main(String[] args) {
        ArrayList<Integer> list =new ArrayList<Integer>();
        list.add(2);
        list.add(4);
        list.add(6);
        list.add(7);
        list.add(9);

        int sum=0;

        for(int i=0;i<list.size();i++){

            sum=sum+list.get(i);


        }

        System.out.println("output="+sum);


    }
}

