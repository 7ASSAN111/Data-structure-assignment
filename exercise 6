import java.util.ArrayList;
import java.util.Scanner;

public class RemoveKeyElements {
    public static ArrayList<Integer> removeKey(ArrayList<Integer> inputList, int key) {
        ArrayList<Integer> resultList = new ArrayList<>();

        for (Integer number : inputList) {
            if (number != key) {
                resultList.add(number);
            }
        }

        return resultList;
    }

    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        ArrayList<Integer> input = new ArrayList<>();

        
        System.out.println("Enter numbers separated by spaces:");
        String line = scanner.nextLine();
        String[] parts = line.trim().split(" ");

        for (String part : parts) {
            try {
                int number = Integer.parseInt(part);
                input.add(number);
            } catch (NumberFormatException e) {
                System.out.println("Invalid input: " + part + " is not a number.");
            }
        }

        
        System.out.print("Enter the key to remove: ");
        int key = scanner.nextInt();

      
        ArrayList<Integer> output = removeKey(input, key);

        System.out.println("Output after removing key " + key + ": " + output);
    }
}
